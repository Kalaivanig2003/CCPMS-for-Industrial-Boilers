APPSCRIPT CODE FOR HISTORY

function doGet(e) { 
  Logger.log( JSON.stringify(e) );  // view parameters
  var result = 'Ok'; // assume success
  if (!e || !e.parameter) {
    result = 'No Parameters';
  }
  else {
    var sheet_id = '1EIwRbapm60D-nsreUe8W6YRSnTSe0r3Tv0jaFuU_F_0';     //To change google Spreadsheet ID
    var sheet = SpreadsheetApp.openById(sheet_id).getActiveSheet();    
    var newRow = sheet.getLastRow() + 1;            
    var rowData = [];
    var Curr_Date = new Date(); // Current date and time
    rowData[0] = Utilities.formatDate(Curr_Date, "Asia/Kolkata", 'HH:mm:ss');          //Enter the Timestamp in column A of EXCEL sheet
    for (var param in e.parameter) {
      Logger.log('In for loop, param=' + param);
      var value = stripQuotes(e.parameter[param]);
      Logger.log(param + ':' + e.parameter[param]);
      switch (param) {
        case 'temperature1': //Parameter
          rowData[1] = value; 					//Enter the Value in column B of excel sheet
          result = 'Written on column B';
          break;
        case 'temperature2': //Parameter
          rowData[2] = value; 					//Enter the Value in column D of excel sheet
          result = 'Written on column C';
          break;

        case 'temperature3': //Parameter
          rowData[3] = value; 					//Enter the Value in column F of excel sheet
          result = 'Written on column D';
          break;
        
        default:
          result = "unsupported parameter";
      }
    }
    Logger.log(JSON.stringify(rowData));
    // Write new row below
    var newRange = sheet.getRange(newRow, 1, 1, rowData.length);
    newRange.setValues([rowData]);
  }
  // Return result of operation
  return ContentService.createTextOutput(result);
}

/**
* Remove leading and trailing single or double quotes
*/
function stripQuotes( value ) {
  return value.replace(/^["']|['"]$/g, "");
}

APPSCRIPT CODE FOR REPLACING

function doGet(e) { 
  Logger.log(JSON.stringify(e));  // view parameters
  var result = 'Ok'; // assume success
  if (!e || !e.parameter) {
    result = 'No Parameters';
  } else {
    var sheet_id = '1S0fjf6qaieL4oahdb9WV9yn4y6izQDCThA7YfJwKQQk'; // Change to your Google Spreadsheet ID
    var sheet = SpreadsheetApp.openById(sheet_id).getActiveSheet();    

    var rowData = [1];
    var Curr_Date = new Date(); // Current date and time
    rowData[0] = Utilities.formatDate(Curr_Date, "Asia/Kolkata", 'HH:mm:ss'); // Enter the Timestamp in column A of the spreadsheet

    var colMapping = {
      'temperature1': 2, // Column B (cell B2)
      'temperature2': 3, // Column C (cell C2)
      'temperature3': 4, // Column D (cell D2)
    };

    for (var param in e.parameter) {
      Logger.log('In for loop, param=' + param);
      var value = stripQuotes(e.parameter[param]);
      Logger.log(param + ':' + e.parameter[param]);
      if (colMapping.hasOwnProperty(param)) {
        var col = colMapping[param];
        sheet.getRange(2, col).setValue(value); // Write the value to the second row (B2, C2, D2)
        result = 'Written to column ' + String.fromCharCode(64 + col); // Convert column number to letter
      } else {
        result = "unsupported parameter";
      }
    }
  }
  // Return result of operation
  return ContentService.createTextOutput(result);
}

/**
 * Remove leading and trailing single or double quotes
 */
function stripQuotes(value) {
  return value.replace(/^["']|['"]$/g, "");
}
